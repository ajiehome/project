{
    "manifest_version": 2,
    "name": "pretty-tab",
    "description": "pretty new tab",
    "version": "1.0",
    "author": "guotingjie@maxthon.net",
    "default_locale": "_lcoales",
    "permissions": [
        "storage",
        "unlimitedStorage",
        "management",
        "livesone",
        "chrome://favicon/",
        "contextMenus"

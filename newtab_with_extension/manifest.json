{
    "manifest_version": 2,

    "name": "pretty-tab",
    "description": "pretty new tab",
    "version": "1.0",
    "author": "YingWu Zhang",
    "permissions": [
        "storage",
        "history",
        "topSites",
        "management",
        "chrome://favicon/",
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
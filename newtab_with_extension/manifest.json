{
    "manifest_version": 2,
    "name": "pretty-tab",
    "description": "pretty new tab",
    "version": "1.0",
    "author": "YingWu Zhang",
    "permissions": [
        "storage",
        "unlimitedStorage",
        "topSites",
        "management",
        "livesone.thumb",
        "livesone.topSites",
        "livesone.searchEngine",
        "chrome://favicon/",
        "contextMenus"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}